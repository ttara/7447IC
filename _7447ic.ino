#include "Arduino.h"

void setup() {
  pinMode(2,OUTPUT);// 02=A
  pinMode(3,OUTPUT);//03=B
  pinMode(4,OUTPUT);//04=c
  pinMode(5,OUTPUT);//05=D   
}
void loop () 
{
  int D=0;
  int C=1;
  int B=0;
  int A=1;
  delay
  digitalWrite(2,A);
  digitalWrite(3,B);
  digitalWrite(4,C);
  digitalWrite(5,D);
  
}

